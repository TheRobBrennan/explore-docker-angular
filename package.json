{
  "name": "explore-docker-angular",
  "version": "0.0.0",
  "description": "A starting point to explore developing Angular apps in Docker containers",
  "author": "Rob Brennan <rob@therobbrennan.com> (therobbrennan.com)",
  "scripts": {
    "start": "npm run docker:up",
    "toh:start": "npm run docker:tour-of-heroes:up",
    "build": "npm run docker:build",
    "toh:build": "npm run docker:tour-of-heroes:build",
    "stop": "npm run docker:down",
    "toh:stop": "npm run docker:tour-of-heroes:down",
    "docker:up": "docker-compose up",
    "docker:build": "docker-compose up --remove-orphans --build --force-recreate",
    "docker:down": "docker-compose down",
    "docker:tour-of-heroes:up": "docker-compose -f docker-compose.tour-of-heroes.yml up",
    "docker:tour-of-heroes:build": "docker-compose -f docker-compose.tour-of-heroes.yml up --remove-orphans --build --force-recreate",
    "docker:tour-of-heroes:down": "docker-compose -f docker-compose.tour-of-heroes.yml down",
    "docker:nuke": "npm run docker:nuke:containers && docker rmi -f $(docker images -q)",
    "docker:nuke:containers": "docker rm $(docker ps -aq)"
  },
  "engines" : {
    "node": ">=12.13.0",
    "npm": ">=6.12.0"
  },
  "keywords": [],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRobBrennan/explore-docker-angular.git"
  },
  "bugs": {
    "url": "https://github.com/TheRobBrennan/explore-docker-angular/issues"
  },
  "homepage": "https://github.com/TheRobBrennan/explore-docker-angular#readme"
}

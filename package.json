{
  "name": "explore-docker-angular",
  "version": "0.0.0",
  "description": "A starting point to explore developing Angular apps in Docker containers",
  "author": "Rob Brennan <rob@therobbrennan.com> (therobbrennan.com)",
  "scripts": {
    "build": "npm run start:clean",
    "start": "npm run start:example:angular-cli-app",
    "start:clean": "npm run start:example:angular-cli-app:clean",
    "stop": "npm run stop:example:angular-cli-app",
    "start:example:angular-cli-app": "cd examples/app && npm start",
    "start:example:angular-cli-app:clean": "cd examples/app && npm run build",
    "stop:example:angular-cli-app": "cd examples/app && npm run stop",
    "start:example:angular-tour-of-heroes": "cd examples/angular-tour-of-heroes && npm start",
    "start:example:angular-tour-of-heroes:clean": "cd examples/angular-tour-of-heroes && npm run build",
    "stop:example:angular-tour-of-heroes": "cd examples/angular-tour-of-heroes && npm run stop",
    "docker:nuke": "docker system prune -a && docker volume prune"
  },
  "engines" : {
    "node": ">=12.13.0",
    "npm": ">=6.12.0"
  },
  "keywords": [],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRobBrennan/explore-docker-angular.git"
  },
  "bugs": {
    "url": "https://github.com/TheRobBrennan/explore-docker-angular/issues"
  },
  "homepage": "https://github.com/TheRobBrennan/explore-docker-angular#readme"
}
